{% if items %}
	<ul class="{{ class }}">
		{% for item in items %}
			<li class="{{ item.classes | join(' ') }}">
				<a class="nav-link" href="{{ item.link|e('esc_url') }}" target="{{ item.target|e('html_attr') ?: '_self' }}">
					{{ item.title|e('wp_kses_post') }}
				</a>
			</li>
		{% endfor %}
	</ul>
{% endif %}
