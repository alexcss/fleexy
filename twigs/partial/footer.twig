					{% if footer.instagram %}
						<a href="{{ footer.instagram|e('esc_url') }}" target="_blank" aria-label="{{ _e('Instagram', 'fp') }}">
							<svg width="40" height="40"><use xlink:href="#icon-instagram"></use></svg>
						</a>
					{% endif %}
					{% if footer.facebook %}
						<a href="{{ footer.facebook|e('esc_url') }}" target="_blank" aria-label="{{ _e('Facebook', 'fp') }}">
							<svg width="40" height="40"><use xlink:href="#icon-facebook"></use></svg>
						</a>
					{% endif %}
					{% if footer.youtube %}
						<a href="{{ footer.youtube|e('esc_url') }}" target="_blank" aria-label="{{ _e('YouTube') }}">
							<svg width="40" height="40"><use xlink:href="#icon-youtube"></use></svg>
						</a>
					{% endif %}
					{% if footer.tiktok %}
						<a href="{{ footer.tiktok|e('esc_url') }}" target="_blank" aria-label="{{ _e('TikTok', 'fp') }}">
							<svg width="40" height="40"><use xlink:href="#icon-tiktok"></use></svg>
						</a>
					{% endif %}
					{% if footer.twitter %}
						<a href="{{ footer.twitter|e('esc_url') }}" target="_blank" aria-label="{{ _e('Twitter', 'fp') }}">
							<svg width="40" height="40"><use xlink:href="#icon-twitter"></use></svg>
