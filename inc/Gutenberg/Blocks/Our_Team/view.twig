<section {{ anchor }} {{ attribute }}>
	<svg class="position-absolute start-0 end-o top-100 w-100"  width="1440" height="32" viewBox="0 0 1440 32" fill="none">
		<path d="M0 0C0 0 438.545 0 720 0C1001.46 0 1440 0 1440 0C1440 0 1001.46 32 720 32C438.545 32 0 0 0 0Z" fill="var(--background-color)"/>
	</svg>
	<div class="container position-relative z-1">
		{% if fields.title %}
			<h2 class="text-uppercase text-center mb-56">{{ fields.title | highlight_text | e('wp_kses_post') }}</h2>
		{% endif %}
		{% if team_members | length > 0 %}
			<div class="row align-items-start justify-content-xl-center mb-56 g-16 g-lg-24 row flex-nowrap fp-no-scroll overflow-x-auto flex-xl-wrap mx-n16 mx-md-n24 mx-xl-n12">

				{% for member in team_members %}
					<div class="col-11 col-sm-10 col-md-5 col-xl-4">
						<div class="fp-team-member h-100 bg-white rounded-16 border border-1 p-16 p-lg-24">
							<div class="h-100 d-flex flex-column flex-md-column align-items-start gap-16 gap-lg-24">
								<div class="d-flex align-items-center gap-8 gap-sm-20">
									<picture class="d-block">
										<source media="(max-width: 480px)"
												srcset="{{ member.thumbnail.src|resize(112, 112, 'center')|towebp }} 1x, {{ member.thumbnail.src|resize(112*2, 112*2, 'center')|towebp }} 1.5x"
												width="112" height="112"/>
										<source media="(max-width: 480px)"
												srcset="{{ member.thumbnail.src|resize(112, 112, 'center')|towebp }} 1x, {{ member.thumbnail.src|resize(112*2, 112*2, 'center')|towebp }} 1.5x"
												type="image/webp" width="112" height="112"/>

										<source media="(max-width: 767px)"
												srcset="{{ member.thumbnail.src|resize(200, 200, 'center')|towebp }} 1x, {{ member.thumbnail.src|resize(200*2, 200*2, 'center')|towebp }} 1.5x"
												width="200" height="200"/>
										<source media="(max-width: 767px)"
												srcset="{{ member.thumbnail.src|resize(200, 200, 'center')|towebp }} 1x, {{ member.thumbnail.src|resize(200*2, 200*2, 'center')|towebp }} 1.5x"
												type="image/webp" width="200" height="200"/>

										<source srcset="{{ member.thumbnail.src|resize(390, 240, 'top')|towebp }} 1x, {{ member.thumbnail.src|resize(390*2, 240*2, 'top')|towebp }} 1.5x"
												type="image/webp" width="390" height="240"/>
										<img class="d-block w-md-100 rounded-16" src="{{ member.thumbnail.src|resize(390, 240, 'top')|tojpg }}" srcset="{{ member.thumbnail.src|resize(390*2, 240*2, 'top')|tojpg }} 1.5x" alt="{{ member.thumbnail.alt|e('html_attr') }}" width="390" height="240">
									</picture>
									<h3 class="font-family-body font-weight-600 mb-0 d-md-none">{{ member.post_title }}</h3>
								</div>
								<div class="h-100 d-flex flex-column align-items-start w-100">
									<h3 class="font-family-body font-weight-600 d-none d-md-block mb-12">{{ member.post_title }}</h3>
									<div class="mb-16">
										<div class="d-flex flex-wrap row-gap-8 gap-16">
											{% for tag in member.terms({taxonomy: 'team_member_tag'}) %}
												<div class="fp-team-member__tag text-uppercase font-weight-600 p-8 rounded-4 border border-blue border-1 border-opacity-25 bg-light-blue">{{ tag.name }}</div>
											{% endfor %}
										</div>
									</div>
									<div class="fp-body-sm mb-24 d-flex flex-wrap row-gap-16 gap-40">
										<div class="d-flex gap-8">
											<div class="fp-team-member__info-icon d-flex justify-content-center align-items-center bg-sandy-brown rounded-8">
												<svg width="24" height="24" viewBox="0 0 24 24" fill="none" >
