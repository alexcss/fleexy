{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "fp-hero",
  "title": "Hero",
  "description": "Hero Block",
  "category": "custom-blocks",
  "icon": "block-default",
  "keywords": [
    "test",
    "quote"
  ],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": [
      "post",
      "page"
    ]
  },
  "styles": [
    {
      "name": "cs-white",
      "label": "White-Blue",
      "isDefault": true
    },
    {
      "name": "cs-light-orange",
      "label": "Light Orange"
    },
    {
      "name": "cs-light-green",
      "label": "Light Green"
    },
    {
      "name": "cs-light-blue",
      "label": "Light Blue"
    }
  ],
  "supports": {
    "align": false,
    "anchor": true,
    "alignContent": false,
    "color": {
      "text": false,
      "background": false,
      "link": false
    },
    "alignText": false,
    "spacing": {
      "customSpacingSize": false,
      "margin": [
        "bottom"
      ]
    },
    "typography": {},
    "fullHeight": false,
    "mode": false
  },
  "attributes": {
    "backgroundColor": {
      "type": "string",
      "default": "purple"
    }
  },
  "example": {
    "attributes": {
      "data": {
        "title": "Your __Health and Wellness__ Space",
        "description": "Take care of your health and well-being. Relax with your family or alone and melt all your troubles away."
      }
    }
  }
}
